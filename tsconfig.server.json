{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node"
    },
    "include": ["server.ts"],
    "exclude": ["node_modules", "**/*.tsx", "**/*.ts?(x)"]
}
  